-- Name: work_shifts; Type: TABLE; Schema: public; Owner: root
--

CREATE TABLE work_shifts (
    id integer NOT NULL,
    kind character varying(255) DEFAULT 'StandardShift'::character varying NOT NULL,
    start_time time without time zone,
    end_time time without time zone,
    meeting_name character varying(255) DEFAULT NULL::character varying,
    shift_date date,
    effective_date date,
    ineffective_date date,
    all_day boolean,
    repeats_every integer DEFAULT 1,
    repeats_on integer DEFAULT 0,
    frequency_type_id integer,
    job_id integer,
    meeting_id integer,
    schedule_id integer,
    shift_id integer,
    weekday_id integer,
    worker_id integer DEFAULT 0,
    actual boolean DEFAULT true,
    training boolean DEFAULT false,
    proposed boolean,
    created_by integer,
    updated_by integer,
    created_at timestamp without time zone,
    updated_at timestamp without time zone,
    offsite boolean DEFAULT false NOT NULL
);


ALTER TABLE work_shifts OWNER TO root;

