    short_name character varying(255) DEFAULT NULL::character varying,
    is_open boolean,
    start_time time without time zone,
    end_time time without time zone,
    open_time time without time zone NOT NULL,
    close_time time without time zone NOT NULL
);


ALTER TABLE weekdays OWNER TO root;

--
-- Name: weekdays_id_seq; Type: SEQUENCE; Schema: public; Owner: root
--

CREATE SEQUENCE weekdays_id_seq
    START WITH 50
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE weekdays_id_seq OWNER TO root;

--
-- Name: weekdays_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: root
--

