    city character varying(30) DEFAULT NULL::character varying,
    state_or_province character varying(15) DEFAULT NULL::character varying,
    postal_code character varying(25) DEFAULT NULL::character varying,
    country character varying(100) DEFAULT NULL::character varying,
    notes text,
    lock_version integer DEFAULT 0 NOT NULL,
    updated_at timestamp without time zone,
    created_at timestamp without time zone,
    created_by integer NOT NULL,
    updated_by integer,
    next_milestone integer DEFAULT 100,
    addr_certified boolean DEFAULT false NOT NULL,
    contract_id integer DEFAULT 1 NOT NULL,
    cashier_created_by integer,
    cashier_updated_by integer,
    fully_covered boolean,
    birthday date,
    volunteer_intern_title character varying(255),
    next_monthly_milestone integer DEFAULT 100,
    exported_to_sf timestamp without time zone,
    household integer DEFAULT 0,
    addr_rejected boolean DEFAULT false
);


