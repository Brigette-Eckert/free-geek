CREATE TABLE shifts (
    id integer NOT NULL,
    type character varying(255) DEFAULT NULL::character varying,
    start_time time without time zone,
    end_time time without time zone,
    meeting_name character varying(255) DEFAULT NULL::character varying,
    shift_date date,
    effective_date date,
    ineffective_date date,
    all_day boolean,
    repeats_every integer DEFAULT 1,
    repeats_on integer DEFAULT 0,
    frequency_type_id integer,
    job_id integer,
    meeting_id integer,
    schedule_id integer,
    weekday_id integer,
    worker_id integer DEFAULT 0,
    actual boolean DEFAULT false,
    training boolean DEFAULT false,
    proposed boolean DEFAULT false NOT NULL,
    created_by integer,
    updated_by integer,
    created_at timestamp without time zone,
    updated_at timestamp without time zone,
    week_1_of_month boolean DEFAULT true NOT NULL,
    week_2_of_month boolean DEFAULT true NOT NULL,
    week_3_of_month boolean DEFAULT true NOT NULL,
    week_4_of_month boolean DEFAULT true NOT NULL,
    week_5_of_month boolean DEFAULT true NOT NULL,
    offsite boolean DEFAULT false NOT NULL,
    repeats_every_months integer DEFAULT 1 NOT NULL,
    repeats_on_months integer DEFAULT 0 NOT NULL,
    week character(1)
);


